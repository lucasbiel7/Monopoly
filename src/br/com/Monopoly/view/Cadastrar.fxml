<AnchorPane id="AnchorPane" fx:id="apPrincipal" prefHeight="466.0" prefWidth="508.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.60" fx:controller="br.com.Monopoly.view.CadastrarController">
    <stylesheets>
        <URL value="@estilo.css" />
    </stylesheets>
   <children>
      <Label fx:id="lbTitulo" layoutX="14.0" layoutY="14.0" prefHeight="54.0" prefWidth="354.0" text="Cadastrar Jogador">
